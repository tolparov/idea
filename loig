package ru.sber.poirot.notifications.kafka

import org.springframework.beans.factory.annotation.Value
import org.springframework.boot.autoconfigure.condition.ConditionalOnProperty
import org.springframework.kafka.core.KafkaTemplate
import org.springframework.stereotype.Component
import ru.sber.poirot.engine.ds.refreshable.delayers.kafka.KafkaUpdateEvent
import ru.sber.toJson
import ru.sber.utils.logger
import java.time.LocalDateTime

@Component
@ConditionalOnProperty("spring.kafka.producer.enable", havingValue = "true", matchIfMissing = false)
class CacheUpdateProducer(
    private val kafkaTemplate: KafkaTemplate<String, String>
) {
    private val log = logger()

    @Value("\${kafka-poirot.topic}")
    private val topic: String = ""

    companion object {
        private const val TRIGGER_NAME = "admin_notifications"
    }

    suspend fun notifyCacheUpdate(notificationId: Long) {
        try {
            val event = KafkaUpdateEvent(
                 notificationId.toString(),
                 LocalDateTime.now(),
                 TRIGGER_NAME
            )

            log.info(" Sending cache update: id={}, topic={}", notificationId, topic)

            kafkaTemplate.send(topic, TRIGGER_NAME, event.toJson())
                .whenComplete { result, ex ->
                    if (ex != null) {
                        log.error(" Failed: {}", ex.message)
                    } else {
                        log.debug(" Sent to {}, offset={}", topic, result?.recordMetadata?.offset())
                    }
                }

        } catch (e: Exception) {
            log.error(" Error: {}", e.message, e)
        }
    }
}
